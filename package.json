{
   "name": "node-ts",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles",
      "build": "npx tsc",
      "lint": "npx eslint . --ext .ts",
      "lint-fix": "npx eslint . --ext .ts --fix",
      "dev": "cross-env NODE_ENV=dev nodemon ./src/index.ts --exec ts-node",
      "decorator": "nodemon ./src/decorators.ts --exec ts-node",
      "start": "ts-node src/index.ts",
      "typecli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
   },
   "husky": {
      "hooks": {
        "pre-commit": "yarn test"
      }
   },
   "author": "Mariano Selvaggi",
   "license": "ISC",
   "devDependencies": {
      "@types/express": "^4.17.9",
      "@types/jest": "^26.0.15",
      "@types/morgan": "^1.9.2",
      "@types/node": "^8.0.29",
      "@types/supertest": "^2.0.10",
      "@typescript-eslint/eslint-plugin": "^4.7.0",
      "@typescript-eslint/parser": "^4.7.0",
      "eslint": "^7.13.0",
      "eslint-plugin-unused-imports": "^1.0.0",
      "hysky": "^1.0.1",
      "jest": "^26.6.3",
      "node-fetch": "^2.6.1",
      "nodemon": "^2.0.6",
      "prettier": "2.1.2",
      "supertest": "^6.0.1",
      "ts-jest": "^26.4.4",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/node-fetch": "^2.5.7",
      "body-parser": "^1.19.0",
      "cross-env": "^7.0.2",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "morgan": "^1.10.0",
      "mysql": "^2.14.1",
      "mysql2": "^2.2.5",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.29",
      "winston": "^3.3.3"
   }
